    <style>
        .ground-highlight-images {
            display: flex;
            /* Use flexbox to align images horizontally */
            flex-wrap: wrap;
            /* Allow images to wrap to the next row on smaller screens */
            justify-content: space-between;
            /* Distribute images evenly across the row */
        }

        .ground-highlight-images img {
            width: 540px;
            /* Set a fixed width for all images */
            height: 400px;
            /* Set a fixed height for all images */
            object-fit: cover;
            /* Ensure the images fit within the fixed dimensions */
            margin: 10px;
            /* Add some margin to separate the images */
            align-items: center;
        }

        .content {
            display: flex;
            align-content: center;
            justify-content: center;
        }

        .letter {
            text-transform: uppercase;
            background-color: #fff;
            background-clip: text;
            text-fill-color: transparent;
            -webkit-background-clip: text;
            -webkit-text-fill-color: transparent;
            display: inline-block;
            font-size: 50px;
            opacity: 0;
            animation: letter-fade-in 2.5s forwards;
            font-weight: bold;
            font-family: bungee, sans-serif;
            letter-spacing: 1px;
        }

        @keyframes letter-fade-in {
            to {
                opacity: 1;
            }
        }

        @media (max-width: 768px) {

            .navbar-brand,
            .navbar-nav {
                margin-left: 0% !important;
            }

            .ground-highlight-images {
                flex-direction: column;
                /* Change to a column layout on mobile screens */
                align-items: center;

            }

            .register {
                margin-top: 2% !important;
            }

            .book-btn a {
                width: 80% !important;
                margin-left: 10% !important;
            }

            .ground-highlight-images img {
                width: 400px;

                /* Set a fixed width for all images */
            }

        }

        @media (max-width: 430px) {
            .ground-highlight-images img {
                width: 360px;
                height: 230px;

            }
        }

        /* navbar css */
        /* zoom effect of the content */
        @keyframes blink {
            0%, 100% { opacity: 1; }
            50% { opacity: 1; }
        }

        @keyframes zoomIn {
            0% { transform: scale(1); }
           
            100% { transform: scale(1); }
        }

        @keyframes zoomOut {
            0% { transform: scale(1); }
           
            100% { transform: scale(1); }
        }

        @keyframes increaseSize {
            0% { font-size: 0.9em; }
            50% { font-size: 1.2em; }
            100% { font-size: 0.9em; }
        }

        a.blink-zoom {
            color: whitesmoke !important; 
            animation: blink 4.5s infinite, zoomIn 4.5s infinite, increaseSize 4.5s infinite;
        }
        /* zoom effect ends */
        .container {
            max-width: 100vw;

        }

        .image-logo {
            width: 8vw;
        }

        .register {
            font-size: 2rem;
            text-align: center;
        }

        @media (max-width:992px) {
            .image-logo {
                width: 9vw
            }
        }

        @media (max-width:458px) {
            .image-logo {
                width: 13vw
            }
        }

        /* footer css */
        .footer {
            margin-top: 6%;
        }

        .image-footer {
            width: 10vw;
        }

        @media (max-width:576px) {
            .mobile-title {
                font-size: 1rem;
                font-weight: bold;
            }
            @keyframes increaseSize {
            0% { font-size: 0.9em; }
            50% { font-size: 0.4em; }
            100% { font-size: 0.9em; }
        }

            .register-c {
